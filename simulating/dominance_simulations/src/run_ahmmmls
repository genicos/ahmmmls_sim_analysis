batch=$1
piece=$2
gens=$3
m=$4
sel=$5
index=$6

m_array=(0.05 0.1 0.2 0.5)
gen_array=(100 200 500 1000)

prop=${m_array[$m]}
generations=${gen_array[$gens]}

./ancestry_hmm -i panels/panel${batch}_${piece}_${index} -s sample -S $prop $generations -P gss_outputs/jesper_out1_0_0 -D > ../outputs/OUT${batch}_${piece}_${gens}_${m}_${sel}_${index}
